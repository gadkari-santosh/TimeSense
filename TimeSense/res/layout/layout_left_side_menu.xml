<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/Black"
    android:gravity="top"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@color/Black"
        android:orientation="vertical" >

        <View
            android:id="@+id/primerdivisor"
            android:layout_width="fill_parent"
            android:layout_height="35dp"
            android:background="@color/Black" />

        <Button
            android:id="@+id/butSettings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/Black"
            android:drawableLeft="@drawable/ic_settings"
            android:fontFamily="normal"
            android:padding="5dp"
            android:text="    Settings"
            android:textColor="@color/DeepSkyBlue"
            android:textSize="15dp"
            android:textStyle="bold" />

        <View
            android:id="@+id/primerdivisor"
            android:layout_width="fill_parent"
            android:layout_height="0.5dp"
            android:background="@color/Gray" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/Black"
            android:drawableLeft="@drawable/ic_help"
            android:fontFamily="normal"
            android:padding="5dp"
            android:text="    Help"
            android:textColor="@color/DeepSkyBlue"
            android:textSize="15dp"
            android:textStyle="bold" />

        <View
            android:id="@+id/primerdivisor"
            android:layout_width="fill_parent"
            android:layout_height="0.5dp"
            android:background="@color/Gray" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/Black"
            android:drawableLeft="@drawable/ic_about_us"
            android:fontFamily="normal"
            android:padding="5dp"
            android:text="    About Us"
            android:textColor="@color/DeepSkyBlue"
            android:textSize="15dp"
            android:textStyle="bold" />

        <View
            android:id="@+id/primerdivisor"
            android:layout_width="fill_parent"
            android:layout_height="0.5dp"
            android:background="@color/Gray" />

        <Button
            android:id="@+id/butContactUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/Black"
            android:drawableLeft="@drawable/ic_contact_us"
            android:fontFamily="normal"
            android:padding="5dp"
            android:text="    Contact Us"
            android:textColor="@color/DeepSkyBlue"
            android:textSize="15dp"
            android:textStyle="bold" />

        <View
            android:id="@+id/primerdivisor"
            android:layout_width="fill_parent"
            android:layout_height="5dp"
            android:background="@color/Gray" />

        <Button
            android:id="@+id/butDialPad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/Black"
            android:drawableLeft="@drawable/ic_action"
            android:padding="5dp"
            android:text="      Dial Pad"
            android:textColor="@color/DeepSkyBlue"
            android:textStyle="bold"
            android:textSize="15dp" />

        <View
            android:id="@+id/primerdivisor"
            android:layout_width="fill_parent"
            android:layout_height="0.5dp"
            android:background="@color/Gray" />

        <Button
            android:id="@+id/logs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/Black"
            android:drawableLeft="@drawable/ic_action_call"
            android:padding="5dp"
            android:text="      Log"
            android:textColor="@color/DeepSkyBlue"
            android:textStyle="bold"
            android:textSize="15dp" />

        <View
            android:id="@+id/primerdivisor"
            android:layout_width="fill_parent"
            android:layout_height="0.5dp"
            android:background="@color/Gray" />

        <Button
            android:id="@+id/contacts"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="@color/Black"
            android:drawableLeft="@drawable/ic_action_person"
            android:padding="5dp"
            android:text="      Contact"
            android:textColor="@color/DeepSkyBlue"
            android:textStyle="bold"
            android:textSize="15dp" />

        <View
            android:id="@+id/primerdivisor"
            android:layout_width="fill_parent"
            android:layout_height="0.5dp"
            android:background="@color/Gray" />

        <Button
            android:id="@+id/preferences"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="@color/Black"
            android:drawableLeft="@drawable/ic_action_web_site"
            android:padding="5dp"
            android:text="      Clock"
            android:textColor="@color/DeepSkyBlue"
            android:textStyle="bold"
            android:textSize="15dp" />
    </LinearLayout>

</LinearLayout>